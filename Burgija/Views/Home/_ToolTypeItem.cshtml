@model ToolType
@{
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />


    <script src="https://kit.fontawesome.com/680ff5430a.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/css/tooltypeitem.css" asp-append-version="true" />

    <style>

        #main {
            margin-bottom: 60px;
        }
    </style>
</head>

<div class="tool-type-box">
    <div class="item-img">
        <img src="data:image/jpeg;base64," alt="">
    </div>
    <div class="item-info">
        <div class="upper-part">
            <p class="item-title">@Model.Name</p>
        </div>
        <div class="middle-part">
            <div class="item-tag">
                <P>NOVO</P>
            </div>
        </div>
        <div class="lower-part">
            <p class="time-added">2 hours ago</p>
            <p class="price">@Model.Price</p>
        </div>
    </div>
</div>
